A,CAAA,CAAA;AACA;AACA;;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,KA/DA;AAgEA,IAAA,KAhEA,mBAiEA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,QAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA;;AADA,sBAEA,MAAA,CAAA,GAAA,IAAA,EAFA;AAAA;AAAA;AAAA;;AAIA,gBAAA,KAAA,CAAA,iFAAA,CAAA;AAJA;AAAA;;AAAA;AAOA,gBAAA,MAPA,GAOA,CAPA;;AAAA;AAAA,sBAOA,MAAA,GAAA,MAAA,CAAA,GAAA,CAAA,MAPA;AAAA;AAAA;AAAA;;AAUA,gBAAA,KAVA,GAUA,CAVA;;AAAA;AAAA,sBAUA,KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,MAVA;AAAA;AAAA;AAAA;;AAYA,gBAAA,GAZA,GAYA,MAAA,CAAA,SAAA,CAAA,KAAA,CAZA;;AAAA,sBAaA,GAAA,CAAA,MAAA,IAAA,QAbA;AAAA;AAAA;AAAA;;AAeA,gBAAA,MAAA,CAAA,EAAA,GAAA,CAAA;;AAfA,sBAgBA,MAAA,CAAA,GAAA,IAAA,MAAA,CAAA,GAAA,CAAA,MAAA,CAhBA;AAAA;AAAA;AAAA;;AAkBA,gBAAA,MAAA,CAAA,KAAA,CAAA,OAAA,IAAA,GAAA,CAAA,KAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,WAAA,IAAA,GAAA,CAAA,SAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,UAAA,IAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,GAAA,CAAA,MAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,IAAA,GAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA,IAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,IAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,GAAA,CAAA,EAAA,GAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,SAAA,IAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,GAAA,CAAA,EAAA,OAAA;AAzBA;AAAA,uBA0BA,KAAA,CAAA,IAAA,CAAA,gCAAA,EAAA,MAAA,CAAA,KAAA,CA1BA;;AAAA;AA2BA,gBAAA,KAAA,CAAA,wBAAA,CAAA;AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,CAAA;;AA5BA;AAUA,gBAAA,KAAA,EAVA;AAAA;AAAA;;AAAA;AAOA,gBAAA,MAAA,EAPA;AAAA;AAAA;;AAAA;AAoCA,oBAAA,MAAA,CAAA,EAAA,IAAA,CAAA,EACA;AACA,kBAAA,KAAA,CAAA,4EAAA,CAAA;AACA,kBAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,KAAA,CAAA,OAAA,GAAA,cAAA;AACA,iBAJA,MAKA,IAAA,MAAA,CAAA,EAAA,IAAA,CAAA,IAAA,MAAA,CAAA,GAAA,IAAA,CAAA,EACA;AACA,kBAAA,KAAA,CAAA,wBAAA,CAAA;AACA;;AA5CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8CA;AA/GA;AAfA,CAAA","sourcesContent":["<template>\r\n<div>\r\n<h1 style=\"text-align:center;margin-left:193px;color:green\">Searching for your favourite Job</h1>\r\n\r\n    <form style=\"text-align:center;margin-left:195px\">\r\n        <img src=\"./p2.jpg\" height=\"200px\" width=\"400px\" alt=\"not available\">\r\n        <div class=\"search-container\">\r\n        <input type=\"text\" placeholder=\"Search company name\" id=\"comName\">\r\n        <input type=\"button\" @click=\"Searchbycomname()\" value=\"Search\"/>\r\n        <table id=\"t1\" class=\"table1\">\r\n          <tr>\r\n            <th>ID</th>\r\n            <th>Company Name</th>\r\n            <th>Job Profession</th>\r\n            <th>Contact Number</th>\r\n            <th>Salary Range</th>\r\n          </tr>\r\n        </table>\r\n        <p style=\"display:none\" id=\"notfound\">Company not found!</p>\r\n        <input type=\"button\" id=\"clear\" @click=\"clear()\" value=\"Clear\" style=\"display:none\"/><br>\r\n        <input type=\"text\" placeholder=\"Enter ID specified in table \" id=\"comid\" >\r\n        <input type=\"button\" @click=\"apply()\" value=\"Apply\"/>\r\n        <router-link style=\"display:none\" to=\"/register\" >confirm</router-link> -->\r\n         <router-link exact v-on:click.native=\"apply()\" to=\"/register\" >Apply</router-link> -->\r\n        <router-link to=\"/register\" id=\"register\" style=\"display:none\">Register Now!</router-link>\r\n        </div>\r\n        \r\n    </form>\r\n    <p class=\"copy\"><b>&copy; Copyright 2020 MyJOB.com</b></p>\r\n\r\n</div>\r\n</template>\r\n<script>\r\nimport Vue from 'vue';\r\nimport VueAxios from 'vue-axios';\r\nimport axios from 'axios';\r\nVue.use(VueAxios,axios)\r\nexport default {\r\n  data(){\r\n    return{\r\n      emplist:[],\r\n      com : \"\",\r\n      index:0,\r\n      type:[],\r\n      userslist:[],\r\n      id1:0,\r\n      st:0,\r\n      empa1:{},\r\n      ids:[],\r\n      st1:0\r\n  }\r\n  },\r\n  methods:{\r\n    async Searchbycomname(){\r\n        await axios.get('http://localhost:3000/employer')\r\n        .then((res) => (this.emplist=res.data));\r\n        await axios.get('http://localhost:3000/users')\r\n        .then((res) => (this.userslist=res.data));\r\n\r\n        this.com=document.getElementById(\"comName\")\r\n        if(this.com.value==\"\")\r\n        {\r\n          alert(\"Please enter name of company!\")\r\n        }\r\n        else{\r\n          // document.getElementById(\"clear\").style.display=\"inline-block\"\r\n          for(this.index = 0; this.index < this.emplist.length; this.index++)\r\n          {\r\n            var ele=this.emplist[this.index];\r\n            if(ele.company==this.com.value)\r\n            {\r\n              document.getElementById(\"notfound\").style.display=\"none\"\r\n              document.getElementById(\"clear\").style.display=\"inline-block\"\r\n              this.type.push(ele.job)\r\n              if((this.type.filter(function(value){return value == ele.job;}).length)==1)\r\n              {\r\n                var table = document.getElementById(\"t1\");\r\n                var row = table.insertRow(-1);\r\n                var cell1 = row.insertCell(0);\r\n                var cell2 = row.insertCell(1);\r\n                var cell3 = row.insertCell(2);\r\n                var cell4 = row.insertCell(3);\r\n                var cell5 = row.insertCell(4);\r\n                cell1.style.height=\"50px\";\r\n                cell1.style.border=\"2px solid #ddd\";\r\n                cell2.style.border=\"2px solid #ddd\";\r\n                cell3.style.border=\"2px solid #ddd\";\r\n                cell4.style.border=\"2px solid #ddd\";\r\n                cell5.style.border=\"2px solid #ddd\";\r\n                cell1.innerHTML = ele.id;\r\n\r\n                cell2.innerHTML = ele.company;\r\n                cell3.innerHTML = ele.job;\r\n                cell4.innerHTML = ele.contact;\r\n                cell5.innerHTML = (\"INR \"+ele.from+\" - \"+ele.to);\r\n                this.ids.push(ele.id);\r\n                // alert(this.ids)\r\n            }}\r\n          }\r\n          if(this.type.length==0){\r\n            document.getElementById(\"notfound\").style.display=\"inline-block\"\r\n        }\r\n        }\r\n        \r\n   },\r\n   clear()\r\n   {\r\n      var table = document.getElementById(\"t1\");\r\n      if(table.rows.length>1)\r\n      {\r\n        for(var j=0;j<table.rows.length;j++){\r\n          table.deleteRow(-1)\r\n        }\r\n      }\r\n      document.getElementById(\"clear\").style.display=\"none\"\r\n   },\r\n   async apply()\r\n   {\r\n     this.id1=document.getElementById(\"comid\").value\r\n     if(this.id1==\"\")\r\n     {\r\n      alert(\"Please search company name in above textbox and enter proper ID given in table!\")\r\n     }\r\n     else{\r\n       for(var index1 = 0; index1 < this.ids.length;index1++)\r\n       {\r\n         \r\n       for(var index = 0; index < this.userslist.length;index++)\r\n          {\r\n            var ele=this.userslist[index];\r\n            if (ele.status==\"active\")\r\n            {\r\n              this.st=2\r\n              if(this.id1==this.ids[index1])\r\n              {\r\n              this.empa1[\"email\"]=ele.email\r\n              this.empa1[\"firstname\"]=ele.firstname\r\n              this.empa1[\"lastname\"]=ele.lastname\r\n              this.empa1[\"gender\"]=ele.gender\r\n              this.empa1[\"age\"]=ele.age\r\n              this.empa1[\"city\"]=ele.city\r\n              this.empa1[\"job\"]=this.emplist[(this.id1-1)].job\r\n              this.empa1[\"company\"]=this.emplist[(this.id1-1)].company\r\n              await axios.post('http://localhost:3000/empapply',this.empa1)\r\n              alert(\"Application Submitted!\")\r\n              this.st1=1\r\n              }\r\n              \r\n              // this.ids\r\n              \r\n          }\r\n          \r\n     }}\r\n     if(this.st==0)\r\n          {\r\n            alert(\"You are not Registered as user click on Register Now to register yourself!\")\r\n            document.getElementById(\"register\").style.display=\"inline-block\"\r\n          }\r\n          else if(this.st==2 && this.st1!=1)\r\n          {\r\n            alert(\"Please enter proper ID\")\r\n          }\r\n     }\r\n   },\r\n   }\r\n   }\r\n</script>\r\n<style scoped>\r\n.copy{\r\n    position:absolute;\r\n    top:170px;\r\n    right:39px;\r\n    color: navy;\r\n    font-size: 19px;\r\n    text-shadow: 1px 1px 2px white;\r\n}\r\n\r\ninput[type=text] {\r\n  padding: 6px;\r\n  margin-top: 8px;\r\n  font-size: 17px;\r\n  border: none;\r\n  margin-left: 31px;\r\n}\r\n\r\n.search-container input[type=button],a{\r\n\r\n  padding: 6px 10px;\r\n  margin-top: 8px;\r\n  margin-right: 16px;\r\n  background: #ddd;\r\n  font-size: 17px;\r\n  border: none;\r\n  cursor: pointer;\r\n  text-decoration: none;\r\n}\r\n\r\n.search-container input[type=button]:hover {\r\n  background: #ccc;\r\n}\r\n.table1 {\r\n  font-family: \"Trebuchet MS\", Arial, Helvetica, sans-serif;\r\n  border-collapse: collapse;\r\n  width: 99%;\r\n  margin-top: 20px;\r\n}\r\n\r\n.table1 td, .table1 th {\r\n  border: 2px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.table1 th {\r\n  padding-top: 12px;\r\n  padding-bottom: 12px;\r\n  text-align: center;\r\n  background-color: #4CAF50;\r\n  color: white;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}