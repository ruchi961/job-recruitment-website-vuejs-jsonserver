OAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,oCAAA,EAAA,IAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,GAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,uDAAA,EAAA,IAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,IAAA,IAAA,IAAA,IAAA,KAAA,IAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,+BAAA,EAAA,IAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,OAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,uDAAA,EAAA,IAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,KAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,4BAAA,EAAA,KAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,QAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,+BAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AACA,UAAA,KAAA,IAAA,CAAA,QAAA,IAAA,KAAA,IAAA,CAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,+BAAA,EAAA,IAAA,CAAA;AACA,QAAA,EAAA,GAAA,OAAA;AACA;;AAGA,UAAA,EAAA,IAAA,MAAA,EACA;AACA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,gCAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EAAA,QAAA,EADA,CACA;;AACA,UAAA,KAAA,CAAA,wBAAA,EAAA,IAAA,CAAA;AACA,SAHA,EAFA,CAOA;AAEA;;AACA,MAAA,CAAA,CAAA,cAAA,GAnDA,CAoDA;AACA;AAvDA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"register\">\r\n    <router-link class=\"format\" to=\"/emplogin\" >Go back to login</router-link> \r\n    <h1>The Registeration page</h1>\r\n    <br />\r\n    <form @submit=\"SubmitData\" method=\"post\">\r\n      <table class=\"data\">\r\n        <tr>\r\n          <td class=\"text\">Enter your first name : </td>\r\n          <td><input name=\"Name\" type=\"text\" placeholder=\"first name\" v-model=\"post.Name\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your Last name : </td>\r\n          <td><input name=\"Last\" type=\"text\" placeholder=\"last name\" v-model=\"post.Last\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your date of join : </td>\r\n          <td><input name=\"doj\" type=\"date\" min=\"2014-07-01\" v-model=\"post.doj\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your Organisation name : </td>\r\n          <td><input name=\"company\" type=\"text\" placeholder=\"Organisation name\" v-model=\"post.company\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter the type of job to recruite :</td>\r\n          <td><input name=\"job\" type=\"text\" placeholder=\"type of job\" v-model=\"post.job\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter the salary range :</td>\r\n          <td><input type=\"number\" name=\"from\" placeholder=\"minimum\" v-model=\"post.from\"></td>\r\n        </tr><br>\r\n          <tr>\r\n          <td></td>\r\n          <td><input id=\"salary\" type=\"number\" name=\"to\"  placeholder=\"maximum\" v-model=\"post.to\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your vacancy limit : </td>\r\n          <td><input name=\"vacancy\" type=\"number\" placeholder=\"Vacancy\" v-model=\"post.vacancy\" /></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your Email Id : </td>\r\n          <td><input name=\"email\" type=\"email\" placeholder=\"Email - Id \" v-model=\"post.email\"></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Enter your Password : </td>\r\n          <td class=\"text\"><input name=\"password\" type=\"password\" placeholder=\"Password\" v-model=\"post.password\"/></td>\r\n        </tr><br>\r\n        <tr>\r\n          <td class=\"text\">Confirm your Password : </td>\r\n          <td><input name=\"re_enter\" type=\"password\" placeholder=\"Confirm Password\" v-model=\"post.re_enter\"/></td>\r\n        </tr><br><br>\r\n        <tr>\r\n          <td><button type=\"submit\" id=\"sub_btn\">Submit</button></td>\r\n          <td><button type=\"reset\" >Reset</button></td>\r\n        </tr>\r\n      </table>\r\n    </form>\r\n    <p class=\"copy\"><b>&copy; Copyright 2020 MyJOB.com</b></p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Vue from 'vue';\r\nimport VueAxios from 'vue-axios';\r\nimport axios from 'axios';\r\nVue.use(VueAxios,axios)\r\n\r\nexport default {\r\n  name: \"Registeration\",\r\n  el :'register',\r\n  data(){\r\n    return {\r\n      post : {\r\n      Name : null,\r\n      Last : null,\r\n      doj : null,\r\n      company : null,\r\n      job : null,\r\n      from : null,\r\n      to : null,\r\n      vacancy : null,\r\n      password : null,\r\n      re_enter :null\r\n      }\r\n    }\r\n  },\r\n  methods:\r\n  {\r\n    SubmitData(e)\r\n    { \r\n      var st=\"true\";\r\n      if(this.post.Name==null || this.post.Last==null){\r\n        alert(\"Please enter your name\",1000);\r\n        st=\"false\";\r\n      }\r\n      if(this.post.doj==null){\r\n        alert(\"Please enter the date of join \",1000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.company==null) {\r\n        alert(\"Please enter your company name !!!\",1000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.job==null) {\r\n        alert(\"Please enter the job requirement for your company !!!\",1000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.from==null || this.post.to==null) {\r\n        alert(\"Please enter the salary range\",1000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.vacancy==null) {\r\n        alert(\"Please enter the number of vacancies for your job !!!\",1000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.email==null) {\r\n        alert(\"Please enter your email Id\",10000);\r\n        st=\"false\";\r\n      }\r\n      if (this.post.password==null) {\r\n        alert(\"Please enter your password !!\");\r\n        st=\"false\";\r\n      }\r\n      if(this.post.password!=this.post.re_enter){\r\n        alert(\"This is not the same password\",1000);\r\n        st=\"false\";\r\n      }\r\n\r\n\r\n      if(st==\"true\")\r\n      {\r\n        // have to give a condition that when will it it register\r\n        this.axios.post('http://localhost:3000/employer',this.post).then((Response)=>{\r\n        console .warn(\"Result\",Response)        // It will show object in the console\r\n        alert(\"Sucessfully Registered\",1000);\r\n        })\r\n\r\n      //here is default value written\r\n      \r\n      }\r\n        e.preventDefault();\r\n        //\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style >\r\n    h1{\r\n        color :rgb(66, 66,66);\r\n        font: 2em sans-serif;\r\n       \r\n    }\r\n.format{\r\n position: absolute;\r\n top:900px;\r\n        left:700px;\r\n}\r\n    .data {\r\n        color:50px black ;\r\n        margin-left: 15%;\r\n    }\r\n\r\n    .text{\r\n      \r\n      font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;\r\n      size: 100px;\r\n      color:rgb(66, 66,66);\r\n    }\r\n    button{\r\n      height: 40px;\r\n      width: 70px;\r\n      cursor: pointer;\r\n    \r\n    }\r\n\r\n    input {\r\n      margin-left: 20px;\r\n    }\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/components"}]}